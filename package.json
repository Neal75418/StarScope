{
  "name": "starscope",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "generate:types": "openapi-typescript http://localhost:8008/api/openapi.json -o src/api/types.generated.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "@tauri-apps/plugin-opener": "^2",
    "framer-motion": "^12.29.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-virtualized-auto-sizer": "^2.0.2",
    "react-window": "^2.2.5",
    "recharts": "^3.6.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,css}": "prettier --write"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.50.0",
    "@tauri-apps/cli": "^2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "openapi-typescript": "^7.13.0",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^7.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.0",
    "vite": "^7.0.4",
    "vitest": "^4.0.17"
  }
}
